{
    "name": "oddity-agency/laravel-filesystem-adapter",
    "description": "filesystem for laravel",
    "type": "library",
    "require": {
        "league/flysystem-aws-s3-v3": "~1.0",
        "aws/aws-sdk-php-laravel": "~3.0",
        "microsoft/azure-storage-file": "^1.0",
        "symfony/http-foundation": "~4.0",
        "league/flysystem-azure-blob-storage": "^0.1.4"
    },
    "autoload": {
        "psr-4": {
            "Filesystem\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filesystem\\FIleStorageServiceProvider"
            ]
        },
        "minimum-stability": "dev"
    },
    "authors": [
        {
            "name": "mradojicic",
            "email": "milos.radojicic@oddity.agency"
        }
    ]
}
